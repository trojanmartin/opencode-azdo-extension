{
  "manifestVersion": 1,
  "id": "opencode-ado-extension",
  "name": "OpenCode Azure DevOps Extension",
  "version": "1.0.0",
  "publisher": "your-publisher-id",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "A custom Azure DevOps task extension built with TypeScript",
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "build",
    "release",
    "azure-pipelines",
    "tasks"
  ],
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/yourusername/opencode-ado"
    },
    "support": {
      "uri": "https://github.com/yourusername/opencode-ado/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/yourusername/opencode-ado"
  },
  "files": [
    {
      "path": "dist"
    },
    {
      "path": "task.json"
    }
  ],
  "contributions": [
    {
      "id": "custom-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "."
      }
    }
  ]
}
